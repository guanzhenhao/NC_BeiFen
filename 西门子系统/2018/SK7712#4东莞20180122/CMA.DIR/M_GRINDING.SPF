PROC M_GRINDING DISPLON
;;**********程序功能**********
;;主程序:
;;各个不同类型机床程序的调用
;;***************************

IF $A_DBB[6]==1
    RING_GROOVE
    GOTOF ENDING
ENDIF

TYPE:
CASE GRIND[0] OF 0 GOTOF TYPE_0 1 GOTOF TYPE_1 2 GOTOF TYPE_2 3 GOTOF TYPE_3 DEFAULT GOTOF TYPE_4

TYPE_0:
SCREW_W
GOTOF ENDING

TYPE_1:
SCREW_N
GOTOF ENDING

TYPE_2:
WORM
GOTOF ENDING

TYPE_3:
EXTERNAL
GOTOF ENDING

TYPE_4:
RING_GROOVE
GOTOF ENDING

ENDING:

RET

