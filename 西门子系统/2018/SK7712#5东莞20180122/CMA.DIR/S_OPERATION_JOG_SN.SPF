PROC S_OPERATION_JOG_SN DISPLOF
;;**********程序功能**********
;;内螺纹手动对刀
;;***************************

G90 G01 C=ACP(0) F=INI[58]
G90 G01 Z=INI[28] F=INI[56]
MSG("X轴正移至对刀起始位")
G90 G01 X=INI[10] F=INI[55]
MSG("Z轴正移至对刀起点")  
G90 G01 Z=INI[2] F=INI[56]

STOPRE
TOOL_SET[5]=$AA_IM[C]

MSG("手动对刀已启动,请进行对刀操作")
FGROUP(Z)
IF INI[0]==1
	G91 Z=-INI[3] X=INI[3]*INI[14]/2 C=-INI[3]/INI[5]*360 F=TECHNOLOGY[30]/4
ELSE
	G91 Z=-INI[3] X=INI[3]*INI[14]/2 C=INI[3]/INI[5]*360 F=TECHNOLOGY[30]/4
ENDIF

RET

