PROC E_COMMON_CROSS_STATUS(REAL ANG_A,REAL ANG_B,REAL RND_VALUE,VAR REAL STATUS_CACU,VAR REAL LENGTH) DISPLOF
;;**********程序功能**********
;;左右第一條縣交叉情況的增加部分長度計算
;;***************************

IF ANG_A+ANG_B<=180
    LENGTH=RND_VALUE/TAN((ANG_A+ANG_B)/2)
    STATUS_CACU=1
ELSE
    LENGTH=RND_VALUE/TAN((360-ANG_A-ANG_B)/2)
    STATUS_CACU=-1
ENDIF

RET

