PROC D_GRINDWHEEL_DIACRT_S(REAL DRESS_POSITION1,REAL DRESS_POSITION2) DISPLOF
;;**********程序功能**********
;;当前砂轮直径计算
;DRESS_POSITION1:初始/当前修整接触位置
;DRESS_POSITION2:初始/当前修整接触位置
;;***************************

IF DRESS_POSITION1>=DRESS_POSITION2
    DRESSER[24]=DRESSER[23]-(DRESS_POSITION1-DRESS_POSITION2)*2
ELSE
    DRESSER[24]=DRESSER[23]
ENDIF

RET

