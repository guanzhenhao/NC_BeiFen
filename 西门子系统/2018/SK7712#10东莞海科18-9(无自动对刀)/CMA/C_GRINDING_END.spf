PROC C_GRINDING_END DISPLOF
;;**********程序功能**********
;;磨削收尾:
;;关闭各种设备
;;***************************

STOPRE
INI[25]=0;退刀计算标志位置0

IF PROCESS[0]==0;如果不是DIY
    G90 G01 C=ACP(INI[27]) F=INI[58];头架在程序结束调整到合适角度
    M9;内磨削冷却关
    M39;外磨削冷却关
    M29;修整冷却关
    M57;退刀关
    M61;对刀关
    F_GRINDWHEEL_STOP;砂轮停止
    F_DRESSWHEEL_STOP;修整轮停止
    IF (INI[70]==1) AND (GRIND[2]<>2);有自动门(0没有/1有)
        M47;罩壳门打开
        WHILE($A_DBB[9]==0);等待罩壳门打开
            G4 F0.2
        ENDWHILE
    ENDIF
ENDIF

RET

