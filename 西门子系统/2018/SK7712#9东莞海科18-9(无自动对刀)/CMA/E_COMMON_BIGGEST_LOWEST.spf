    ENDIF
    IF POINT_BIGGEST<POINT_5
        POINT_BIGGEST=POINT_5
    ENDIF
    IF POINT_BIGGEST<POINT_6
        POINT_BIGGEST=POINT_6
    ENDIF
    IF POINT_BIGGEST<POINT_7
        POINT_BIGGEST=POINT_7
    ENDIF
    IF POINT_BIGGEST<POINT_8
        POINT_BIGGEST=POINT_8
    ENDIF
    IF POINT_BIGGEST<POINT_9
        POINT_BIGGEST=POINT_9
    ENDIF
    IF POINT_BIGGEST<POINT_10
        POINT_BIGGEST=POINT_10
    ENDIF
ELSE
    IF POINT_BIGGEST>POINT_1
        POINT_BIGGEST=POINT_1
    ENDIF
    IF POINT_BIGGEST>POINT_2
        POINT_BIGGEST=POINT_2
    ENDIF
    IF POINT_BIGGEST>POINT_3
        POINT_BIGGEST=POINT_3
    ENDIF
    IF POINT_BIGGEST>POINT_4
        POINT_BIGGEST=POINT_4
    ENDIF
