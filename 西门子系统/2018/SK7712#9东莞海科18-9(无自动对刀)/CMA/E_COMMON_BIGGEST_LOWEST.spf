        POINT_BIGGEST=POINT_5
    ENDIF
    IF POINT_BIGGEST>POINT_6
        POINT_BIGGEST=POINT_6
    ENDIF
    IF POINT_BIGGEST>POINT_7
        POINT_BIGGEST=POINT_7
    ENDIF
    IF POINT_BIGGEST>POINT_8
        POINT_BIGGEST=POINT_8
    ENDIF
    IF POINT_BIGGEST>POINT_9
        POINT_BIGGEST=POINT_9
    ENDIF
    IF POINT_BIGGEST>POINT_10
        POINT_BIGGEST=POINT_10
    ENDIF
ENDIF

;计算最小值
IF GRIND[0]<>1;不是内螺纹
    IF POINT_LOWEST>POINT_1
        POINT_LOWEST=POINT_1
    ENDIF
    IF POINT_LOWEST>POINT_2
        POINT_LOWEST=POINT_2
    ENDIF
    IF POINT_LOWEST>POINT_3
        POINT_LOWEST=POINT_3
    ENDIF
    IF POINT_LOWEST>POINT_4
        POINT_LOWEST=POINT_4
    ENDIF
    IF POINT_LOWEST>POINT_5
        POINT_LOWEST=POINT_5
    ENDIF
    IF POINT_LOWEST>POINT_6
        POINT_LOWEST=POINT_6
    ENDIF
    IF POINT_LOWEST>POINT_7
        POINT_LOWEST=POINT_7
    ENDIF
    IF POINT_LOWEST>POINT_8
        POINT_LOWEST=POINT_8
    ENDIF
    IF POINT_LOWEST>POINT_9
        POINT_LOWEST=POINT_9
    ENDIF
    IF POINT_LOWEST>POINT_10
        POINT_LOWEST=POINT_10
    ENDIF
ELSE
    IF POINT_LOWEST<POINT_1
        POINT_LOWEST=POINT_1
    ENDIF
    IF POINT_LOWEST<POINT_2
        POINT_LOWEST=POINT_2
    ENDIF
    IF POINT_LOWEST<POINT_3
        POINT_LOWEST=POINT_3
    ENDIF
    IF POINT_LOWEST<POINT_4
        POINT_LOWEST=POINT_4
    ENDIF
