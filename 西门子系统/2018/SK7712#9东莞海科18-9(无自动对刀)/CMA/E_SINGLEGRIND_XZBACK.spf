PROC E_SINGLEGRIND_XZBACK(REAL POSITION) DISPLOF
;;**********程序功能**********
;;单向磨削后x后退情况
;POSITION:X后退位置
;;***************************

MSG("砂轮退出工件")
G90 G01 X=POSITION-$AC_DRF[X] F=PROCESS[11]*10
INI[47]=0;磨削中不正常退出标记
MSG("台面到磨削起点")
IF INI[84]==0;内螺纹是否螺旋退出工件(0否1是)
    ;G01 Z=INI[6] F=INI[56]
ELSE
    F_ZC_START(INI[7],INI[6],INI[5],50*INI[5],0)
ENDIF

RET

