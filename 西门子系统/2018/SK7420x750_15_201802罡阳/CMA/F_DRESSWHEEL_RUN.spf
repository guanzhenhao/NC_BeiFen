PROC F_DRESSWHEEL_RUN(REAL SPEED) DISPLOF
;;**********程序功能**********
;;修整轮启动
;SPEED:转速
;;***************************

IF $A_DBB[13]==0;修整轮当前未启动
    INI[71]=1;启动等待转速到位延时时间
ELSE
    INI[71]=0.2
ENDIF

IF (INI[37]==0) OR (INI[37]==2);修整轮类型选择
    R297=SPEED
    G4 F0.2
    M63;修整轮启动
ELSE
    IF (INI[36]==0) OR (INI[36]==2)
        M1=3 S1=SPEED
    ELSE
        M2=3 S2=SPEED
    ENDIF
ENDIF

G4 F=INI[71];转速等待时间

RET

