PROC S_WARE_ROLLING DISPLOF


DEF AXIS AXIVER

AXIVER=AXNAME(AXIS_VER)

IF DRESSER[18]+DRESSER[19]+DRESSER[20]==0
    RET
ENDIF

G90 G1 AX[AXIVER]=WHEEL[10] F=DRESSER[10]
G4 F=WHEEL[18]
IF GRIND[0]<>1
    G91 G1 AX[AXIVER]=1
ELSE
    IF DRESSER[37]==0
        G91 G1 AX[AXIVER]=-1
    ELSE
        G91 G1 AX[AXIVER]=1
    ENDIF
ENDIF
DRESSER[5]=1

RET

