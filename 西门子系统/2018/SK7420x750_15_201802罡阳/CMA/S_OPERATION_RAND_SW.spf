PROC S_OPERATION_RAND_SW DISPLOF


DEF REAL DR1,DR2,DR3,DR4

CASE TOOL_SET[19] OF 0 GOTOF DDD0 1 GOTOF DDD1 2 GOTOF DDD2 DEFAULT GOTOF DDD1

DDD0:
MSG("正在开至工件中点")
G01 G90 Z=INI[8] F=2000
GOTOF DDD2

DDD1:
MSG("正在开至工件起点")
G01 G90 Z=INI[6] F=2000
GOTOF DDD2

DDD2:
STOPRE
TOOL_SET[0]=$AA_IM[Z]
DR1=ABS(TOOL_SET[0]-INI[6])
DR2=DR1/INI[5]
DR3=(DR2-TRUNC(DR2))*360
DR4=TOOL_SET[4]+DR3
IF DR4>=360
	DR4=DR4-360
ENDIF
MSG("头架到位中")
G90 G01 C=DR4 F=3600

MSG("X轴开至对刀起始位")
G01 G90 X=INI[10] F=800
MSG("砂轮正在进入工件螺纹")
G01 X=PROCESS[4]+ABS(TOOL_SET[0]-INI[6])*INI[14]/2 F=50
STOPRE

MSG("二次对刀已启动,请进行对刀操作")
FGROUP(Z)
IF INI[0]==1
	G91 Z=(INI[7]-TOOL_SET[0]) X=ABS(INI[7]-TOOL_SET[0])*INI[14]/2 C=ABS((INI[7]-TOOL_SET[0])/INI[5]*360) F=50
ELSE
	G91 Z=(INI[7]-TOOL_SET[0]) X=ABS(INI[7]-TOOL_SET[0])*INI[14]/2 C=ABS((INI[7]-TOOL_SET[0])/INI[5]*360) F=50 
ENDIF

RET

