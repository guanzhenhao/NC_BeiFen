PROC S_OPERATION_JOG_SN DISPLOF


G90 G01 Z=INI[28] F1500
MSG("X轴正移至对刀起始位")
G90 G01 X=INI[10] F2000
MSG("Z轴正移至对刀起点")  
G90 G01 Z=INI[2] F=1500
TOOL_SET[5]=$AA_IM[C]

MSG("手动对刀以启动,请进行对刀操作")
FGROUP(Z)
IF INI[0]==1
	G91 Z=-INI[3] X=INI[3]*INI[14]/2 C=-INI[3]/INI[5]*360 F=50
ELSE
	G91 Z=-INI[3] X=INI[3]*INI[14]/2 C=INI[3]/INI[5]*360 F=50
ENDIF


RET

