PROC RING_GROOVE DISPLON


DRFOF
STOPRE

IF INI[36]==0
    R299=2000
    G4 F0.2
    M13
ELSE
    M1=3 S1=2000
    WAITS
ENDIF
MSG("等待砂轮转速到位")
G4 F5


C_CYCLE:
MSG("头架旋转中...")
G91 C=360000 F=7200
IF $A_DBB[2]==1
    GOTOF RING_END
ENDIF
GOTOB C_CYCLE

RING_END:
DRFOF
M9
IF INI[36]==0
    M15
ELSE
    M1=5
ENDIF
G4 F1

RET

