PROC EXCIRCLE_LINESPEED_GRIND SBLOF DISPLOF
;砂轮速度

DEF REAL WHEEL_RPM;砂轮转速

WHEEL_RPM= PROCESS[23]*60000/($PI*DRESSER[91]);计算外圆磨过程中砂轮转速

R298=WHEEL_RPM
