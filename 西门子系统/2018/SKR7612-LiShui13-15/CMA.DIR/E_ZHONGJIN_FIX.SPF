PROC E_ZHONGJIN_FIX(REAL VALUE) DISPLOF
;;**********程序功能**********
;;中径微调:

;;***************************

IF GRIND[0]<>1
	IF (VALUE>-1)AND(VALUE<1)
		PROCESS[4]=PROCESS[4]+VALUE/2
		PROCESS[13]=PROCESS[13]+VALUE/2
	ENDIF
ELSE
	IF (VALUE>-1)AND(VALUE<1)
		PROCESS[4]=PROCESS[4]-VALUE/2
		PROCESS[13]=PROCESS[13]-VALUE/2
	ENDIF
ENDIF

RET

