PROC C_DRESS_START DISPLOF
;;**********程序功能**********
;;报警及参数初始化:
;;参数初始化，提前判断本次修整是否砂轮直径过小
;;***************************
DEF INT XB
DEF REAL LINSHI

DRESSER[18]=0
DRESSER[19]=0
DRESSER[20]=0

XB=101
R110=DRESSER[35]
LINSHI=R110
WHILE(XB<=110)
	IF LINSHI<R[XB]
		LINSHI=R[XB]
	ENDIF
	XB=XB+1
ENDWHILE
IF DRESSER[24]-(DRESSER[11]*DRESSER[14])*2-(DRESSER[12]*DRESSER[15])*2<LINSHI
	DRESSER[6]=2
	GOTOF ENDING_A
ENDIF

ENDING_A:
RET

