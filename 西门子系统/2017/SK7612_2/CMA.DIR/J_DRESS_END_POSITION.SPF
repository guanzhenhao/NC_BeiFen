PROC J_DRESS_END_POSITION DISPLOF
;;**********程序功能**********
;;修整结束移动位置
;;***************************

MSG("砂轮架回退到修整位中")
IF PROCESS[57]==1
    G90 G01 AX[AXNAME(AXIS_EW)]=DRESSER[88]+2 F=INI[105]
ELSE
    G90 G01 AX[AXNAME(AXIS_EW)]=DRESSER[88]-2 F=INI[55]
ENDIF
MSG("台面离开中")
IF (GRIND[5]==1) AND ((PROCESS[45]+PROCESS[44])>=1)
    G90 G01 Z=DRESSER[108]+10 F=INI[56] 
ELSE
    G90 G01 Z=INI[7] F=INI[56] 
ENDIF

RET

