PROC S_DRESS_SHAPE DISPLON
;;**********程序功能**********
;;齿形选择
;;***************************

IF DRESSER[18]+DRESSER[19]+DRESSER[20]==0
    RET
ENDIF

CASE DRESSER[4] OF 0 GOTOF SHAPE_0 1 GOTOF SHAPE_1 DEFAULT GOTOF SHAPE_2

SHAPE_0:
S_TRIANGLE
DRESSER[5]=1
RET

SHAPE_1:
S_TRAPEZOIDAL
DRESSER[5]=1
RET

SHAPE_2:
S_DOUBLE_ARC
DRESSER[5]=1
RET

