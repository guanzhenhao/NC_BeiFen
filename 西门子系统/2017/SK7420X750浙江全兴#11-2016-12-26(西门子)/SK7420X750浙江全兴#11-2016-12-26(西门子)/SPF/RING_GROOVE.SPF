PROC RING_GROOVE DISPLON


DRFOF
STOPRE

IF (INI[36]==0) OR (INI[36]==2)
    R299=1500
    G4 F0.2
    M13
ELSE
    M1=3 S1=1500
    WAITS
ENDIF
MSG("等待砂轮转速到位")
G4 F5


C_CYCLE:
MSG("头架旋转中...")
G91 C=360000 F=INI[58]
IF $A_DBB[2]==1
    GOTOF RING_END
ENDIF
GOTOB C_CYCLE

RING_END:
DRFOF
M9
IF (INI[36]==0) OR (INI[36]==2)
    M15
ELSE
    M1=5
ENDIF
G4 F1

RET

